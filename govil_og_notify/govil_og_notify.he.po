# Hebrew translation of Gov IL OG Notifications

msgid "Configure notifications for"
msgstr "הגדר התראות ל"

msgid "Manage Notifications"
msgstr "ניהול התראות"

msgid "Manage notifications in group."
msgstr ".ניהול התראות על עידכוני תוכן בקבוצה"

msgid "Group"
msgstr "קבוצה"

msgid "Users"
msgstr "משתמשים"

msgid "All members in the group"
msgstr "כל חברי הקבוצה"

msgid "You"
msgstr "את/ה"

msgid "Do you want to change setting for you or for all members?"
msgstr "האם את/ה משנה הגדרות לך או לכל חברי קבוצתך?"

msgid "Set the content types you want to send."
msgstr "בחר/י את סוגי התוכן שעליהם את/ה מעוניין/ת לקבל התראות."

msgid "Frequency"
msgstr "תדירות"

msgid "Monthly"
msgstr "חודשי"

msgid "What is the frequency you want?"
msgstr "באיזו תדירות תרצה/י לקבל התראות?"

msgid "New"
msgstr "חדש"

msgid "Do you want to get notification about new or updated content?"
msgstr "האם תרצה/י לקבל התראה על תוכן חדש או על תוכן שעודכן?"

msgid "Your configuration has been saved."
msgstr "ההגדרות נשמרו בהצלחה."

msgid "has been created"
msgstr "נוצר"

msgid "has been updated"
msgstr "עודכן"

msgid "Updates for you"
msgstr "עידכונים בשבילך"

msgid "Notifications about new content"
msgstr "התראות על תוכן חדש או מעודכן"
